{
  "name": "filestack-react",
  "version": "3.1.0",
  "main": "dist/filestack-react.js",
  "description": "React component wrapper for filestack-js",
  "scripts": {
    "test": "jest --no-cache --coverage",
    "test:Watch": "jest --watch --no-cache --coverage",
    "build": "rimraf dist && webpack --colors -p --display-error-details",
    "lint": "eslint src/**/*.js src/**/*.jsx tests/**/*spec.js && tsc --noEmit",
    "release": "npm test && npm run build && standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filestack/filestack-react.git"
  },
  "keywords": [
    "filestack-react",
    "filestack",
    "filepicker",
    "react",
    "uploader",
    "upload",
    "file",
    "image",
    "ssr",
    "server"
  ],
  "author": "ZeroCho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filestack/filestack-react/issues"
  },
  "homepage": "https://github.com/filestack/filestack-react#readme",
  "dependencies": {
    "filestack-js": "3.x.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-stage-1": "^7.8.3",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/runtime": "^7.10.5",
    "@types/react": "^16.9.43",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "browser-resolve": "^1.11.3",
    "css-loader": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "imports-loader": "^1.1.0",
    "jest": "^26.1.0",
    "jest-cli": "^26.1.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "standard-version": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.8",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__coverage__",
      "<rootDir>/__tests__/__mocks__",
      "<rootDir>/node_modules"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": "\\.(spec|e2e|snapshot)\\.js$",
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/tests/__coverage__/",
    "coverageReporters": [
      "json",
      "html",
      "text"
    ],
    "moduleNameMapper": {
      "mock-data": "<rootDir>/tests/__mocks__/mockData.js",
      "^.+\\.(scss|css|svg|png)$": "identity-obj-proxy",
      "src": "<rootDir>/src"
    }
  }
}
