{
  "name": "filestack-react",
  "version": "2.0.4",
  "main": "dist/filestack-react.js",
  "description": "React component wrapper for filestack-js",
  "scripts": {
    "test": "jest --watch --no-cache --coverage",
    "build": "rimraf dist && webpack --colors -p --display-error-details",
    "lint": "eslint src/**/*.js src/**/*.jsx tests/**/*spec.js",
    "release": "npm test && npm run build && standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/filestack/filestack-react.git"
  },
  "keywords": [
    "filestack-react",
    "filestack",
    "filepicker",
    "react",
    "uploader",
    "upload",
    "file",
    "image",
    "ssr",
    "server"
  ],
  "author": "ZeroCho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/filestack/filestack-react/issues"
  },
  "homepage": "https://github.com/filestack/filestack-react#readme",
  "dependencies": {
    "filestack-js": "^2.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-1": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "imports-loader": "^0.8.0",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "prop-types": "^15.7.1",
    "raw-loader": "^1.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-scripts": "^2.1.5",
    "react-test-renderer": "^16.8.1",
    "rimraf": "^2.6.3",
    "standard-version": "^5.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/__coverage__",
      "<rootDir>/__tests__/__mocks__",
      "<rootDir>/node_modules"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testRegex": "\\.(spec|e2e|snapshot)\\.js$",
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/tests/__coverage__/",
    "coverageReporters": [
      "json",
      "html",
      "text"
    ],
    "browser": true,
    "moduleNameMapper": {
      "mock-data": "<rootDir>/tests/__mocks__/mockData.js",
      "^.+\\.(scss|css|svg|png)$": "identity-obj-proxy",
      "src": "<rootDir>/src"
    }
  }
}
